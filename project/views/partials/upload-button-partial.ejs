    <tr>
        <td>Chhattisgarh</td>
        <td><input type="file" id="ChhattisgarhFileInput"></td>
        <td>
            <button id="ChhattisgarhButton" onclick="fetchFromArrays(0)"
                    class="generate_button">FillForm
            </button>
        </td>
    </tr>
