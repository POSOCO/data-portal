<% if(user != null && (user.username == "cseb" || user.username == "dd" || user.username == "dnh" || user.username == "esil" || user.username == "geb" || user.username == "goa" || user.username == "mpseb" || user.username == "mseb"|| user.username == "cpccv")){ %>
<table class="table" style="max-width: 300px;">
